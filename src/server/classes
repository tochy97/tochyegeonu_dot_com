class Date {
    constructor(id, {day, month, year, data}) {
        this.id = id;
        this.day = day;
        this.month = month;
        this.year = year;
        this.data = data;
    }
}

class Week {
    constructor(id, {value, days, month, year}){
        this.id = id; 
        this.value = value;
        this.days = days;
        this.month = month;
        this.year = year;
    }
}

class Month {
    constructor(id, {name, value, weeks, year}){
        this.id = id; 
        this.name = name;
        this.value = value;
        this.weeks = weeks;
        this.year = year;
    }
}

class Year {
    constructor(id, {value, months, leap}){
        this.id = id; 
        this.value = value;
        this.months = months;
        this.leap = leap;
    }
}

class Life {
    constructor(id, {name, year}){
        this.id = id; 
        this.name = name;
        this.year = year;
    }
}